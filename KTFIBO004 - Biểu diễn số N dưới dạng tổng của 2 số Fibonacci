#include<bits/stdc++.h>
#define ll long long
using namespace std;
ll F[32];
int main(){
    F[0]=0;
    F[1]=1;
    for(int i=2;i<=33;i++){
        F[i]=F[i-1]+F[i-2];
    }
    int t;
    cin>>t;
    while(t--){
        ll n;
        cin>>n;
        bool check=false;
        for(int i=0;i<=33&&!check;i++){
            for(int j=i+1;j<=33&&!check;j++){
                if(F[i]+F[j]==n){
                    check=true;
                    cout<<F[i]<<" "<<F[j]<<endl;
                }
            }
        }
        if(!check)	cout<<-1<<endl;
    }
    return 0;
}
