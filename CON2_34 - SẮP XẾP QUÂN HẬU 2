#include<bits/stdc++.h>
#define ll long long
using namespace std;
int n=8;
ll a[10][10];
bool hang[10],cot[10],cheoxuoi[20],cheonguoc[20];
ll path;
void Try(int i,ll sum){
    for(int j=1;j<=n;j++){
        if(!hang[i]&&!cot[j]&&!cheoxuoi[i+j-1]&&!cheonguoc[i-j+n]){
            hang[i]=cot[j]=cheoxuoi[i+j-1]=cheonguoc[i-j+n]=true;
            if(i==n){
        		path=max(path,sum+a[i][j]);
    		}
            else Try(i+1,sum+a[i][j]);
            hang[i]=cot[j]=cheoxuoi[i+j-1]=cheonguoc[i-j+n]=false;
        }
    }
}
int main(){
    int t;
    cin>>t;
    while(t--){
        for(int i=1;i<=8;i++){
            for(int j=1;j<=8;j++){
                cin>>a[i][j];
            }
        }
       	path=0;
        Try(1,0);
        cout<<path<<endl;
    }
    return 0;
}
