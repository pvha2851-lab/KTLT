#include<bits/stdc++.h>
using namespace std;
const int INF=1e6;
vector<int> isprime(INF,1);
vector<int> prime;
void sangnt(){
	for(int i=2;i<=sqrt(INF);i++){
		if(isprime[i]){
			for(int j=i*i;j<=INF;j+=i){
				isprime[j]=0;
			}
		}
	}
	for(int i=2;i<=INF;i++){
		if(isprime[i]){
			prime.push_back(i);
		}
	}
}
bool chuso(int n){
	while(n>0){
		int r=n%10;
		if(r!=2&&r!=3&&r!=5&&r!=7)
			return false;
		n/=10;
	}
	return true;
}
int main(){
	sangnt();
	int t;
	cin>>t;
	while(t--){
		int n;
		cin>>n;
		int cnt=0;
		for(int i=0;i<prime.size();i++){
			if(prime[i]>n)	break;
			if(chuso(prime[i]))	cnt++;
		}
		cout<<cnt<<endl;
	}
	return 0;
}
