#include<bits/stdc++.h>
#define ll long long
using namespace std;
int main(){
	int t;
	cin>>t;
	while(t--){
		map<ll,ll> cnt;
		ll n;
		cin>>n;
		while(n%2==0){
			cnt[2]++;
			n=n/2;
		}
		for(ll i=3;i<=sqrt(n);i+=2){
			while(n%i==0){
				cnt[i]++;
				n=n/i;
			}
		}
		if(n>1)	cnt[n]++;
		cout<<cnt.size()<<endl;
		for(auto x:cnt){
			cout<<x.first<<" "<<x.second<<endl;
		}
	}
	return 0;
}
