#include<bits/stdc++.h>
#define ll long long
using namespace std;
ll INF=1e9;
vector<ll> F;
void sangfibo(){
    F.push_back(1);
    F.push_back(2);
    while(1){
        ll x=F[F.size()-1]+F[F.size()-2];
        if(x>INF)	break;
        F.push_back(x);
    }
}
bool isF(ll n){
    for(int i=0;i<F.size();i++){
        if(F[i]==n)	return true;
    }
    return false;
}
int main(){
     sangfibo();
    int t;
    cin>>t;
    while(t--){
        ll n;
        cin>>n;
        n++;
        while(isF(n)) n++;
        cout<<n<<endl;
    }
    return 0;
}
