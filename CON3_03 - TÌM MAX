#include<bits/stdc++.h>
#define ll long long
using namespace std;
const ll mod=1e9+7;
int main(){
    int t;
    cin>>t;
    while(t--){
        int n;
        cin>>n;
        vector<ll> a;
        for(int i=0;i<n;i++){
            ll x;cin>>x;
            a.push_back(x);
        }
        sort(a.begin(),a.end());
        ll sum=0;
        for(int i=0;i<a.size();i++){
            sum+=a[i]*i;
        }
        cout<<sum<<endl;
    }
    return 0;
}
