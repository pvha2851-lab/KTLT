#include<bits/stdc++.h>
#define ll long long
using namespace std;
const ll mod=1e9+7;
ll n,k;
struct matran{
	ll A[15][15];
	friend matran operator * (matran x,matran y){
		matran z;
		for(int i=0;i<n;i++){
			for(int j=0;j<n;j++){
				z.A[i][j]=0;
				for(int tmp=0;tmp<n;tmp++){
					z.A[i][j]+=x.A[i][tmp]*y.A[tmp][j];
					z.A[i][j]%=mod;
				}
			}
		}
		return z;
	}
};
matran binpow(matran a,ll y){
	if(y==1)	return a;
	matran X=binpow(a,y/2);
	if(y%2==1)	return X*X*a;
	else	return X*X;
}
int main(){
	int t;
	cin>>t;
	while(t--){
		matran x;
		cin>>n>>k;
		for(int i=0;i<n;i++){
			for(int j=0;j<n;j++)
				cin>>x.A[i][j];
		}
		matran res=binpow(x,k);
		ll sum_cheophu=0;
		for(int i=0;i<n;i++){
			sum_cheophu+=res.A[i][n-1-i];
			sum_cheophu%=mod;
		}
		cout<<sum_cheophu<<endl;
	}
	return 0;
}
