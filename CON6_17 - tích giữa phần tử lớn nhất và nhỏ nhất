#include<bits/stdc++.h>
#define ll long long
using namespace std;
int main(){
    int t;
    cin>>t;
    while(t--){
        int n,m;
        cin>>n>>m;
        vector<int> a(n);
        vector<int> b(m);
        for(int i=0;i<n;i++){
            cin>>a[i];
        }
        for(int j=0;j<m;j++){
            cin>>b[j];
        }
        int pos1=max_element(a.begin(),a.end())-a.begin();
        int pos2=min_element(b.begin(),b.end())-b.begin();
        ll mul=a[pos1]*b[pos2];
        cout<<mul<<endl;
    }
    return 0;
}
