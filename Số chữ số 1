#include<bits/stdc++.h>
using namespace std;
int check(int n){
    int dem=0;
    while(n>0){
        int r=n%10;
        if(r==1)	dem++;
        n=n/10;
    }
    return dem;
}
int main(){
    int t;
    cin>>t;
    while(t--){
        int n;
        cin>>n;
        int cnt=0;
        for(int i=1;i<=n;i++){
            cnt+=check(i);
        }
        cout<<cnt<<endl;
    }
    return 0;
}
