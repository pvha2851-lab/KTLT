#include<bits/stdc++.h>
using namespace std;
int n;
vector<vector<int>> res;
void Try(int i,int m,vector<int> v){
	if(m==1){
		res.push_back(v);
		return;
	} 
	for(int j=i;j<=m;j++){
		if(m%j==0&&j!=n){
			v.push_back(j);
			Try(j,m/j,v);
			v.pop_back(); 
		}
	}
}
int main(){
	int t;
	cin>>t;
	while(t--){
		cin>>n;
		res.clear(); 
		vector<int> v;
		Try(2,n,v);
		cout<<res.size()<<endl; 
		for(int i=0;i<res.size();i++){
			for(int j=0;j<res[i].size();j++){
				cout<<res[i][j]<<" ";
			}
			cout<<endl;
		}
	}
	return 0;
}
