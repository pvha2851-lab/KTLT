#include<bits/stdc++.h>
#define ll long long 
using namespace std;
bool prime(int n){
	if(n<2)	return false;
	for(int i=2;i<=sqrt(n);i++){
		if(n%i==0)
			return false;
	}
	return true;
}
vector<ll> perfect;
void sangperfect(){
	for(int p=1;p<=33;p++){
		if(prime(p))
		{
			ll x1=pow(2,p-1);
			ll x2=pow(2,p)-1;
			ll x=x1*x2;
			perfect.push_back(x);
		}
	}
}
int main(){
	sangperfect();
	int t;
	cin>>t;
	while(t--){
		ll n;
		cin>>n;
		bool check=false;
		for(int i=0;i<perfect.size();i++){
			if(n==perfect[i]){
				check=true;
				cout<<1<<endl;
				break;
			}
		}
		if(!check){
			cout<<0<<endl;
		}
	}
	return 0;
} 
