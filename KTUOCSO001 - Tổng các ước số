#include<bits/stdc++.h>
#define ll long long
using namespace std;
ll sum_uoc(ll n){
	if(n==1)	return 0;
	ll sum=0;
	for(ll i=1;i<=sqrt(n);i++){
		if(n%i==0){
			sum+=i;
			if(n/i!=i&&n/i!=n){
				sum+=n/i;
			}
		}
	}
	return sum;
}
int main(){
	int t;
	cin>>t;
	while(t--){
		ll n;
		cin>>n;
		cout<<sum_uoc(n)<<endl;
	}
	return 0;
}
