#include<bits/stdc++.h>
#define ll long long
using namespace std;
vector<ll> C(32,0);
void catalan(){
	C[0]=1;
	for(int i=1;i<=31;i++){
		for(int k=0;k<i;k++){
			C[i]+=C[k]*C[i-1-k];
		}
	}
}
int main(){
	catalan();
	int t;
	cin>>t;
	while(t--){
		int n;
		cin>>n;
		cout<<C[n]<<endl;
	}
	return 0;
} 
