#include<bits/stdc++.h>
#define ll long long
using namespace std;
ll dmax;
ll uoc_max(ll n){
	ll ans=0;
	while(n%2==0){
		ans=2;
		n=n/2;
	} 
	while(n%3==0){
		ans=3;
		n=n/3;
	}
	for(ll i=5;i<=sqrt(n);i+=6){
		while(n%i==0){
			ans=i;
			n=n/i;
		}
		while(n%(i+2)==0){
			ans=i+2;
			n=n/(i+2);
		}
	}
	if(n>4)	ans=n;
	return ans;
}
int main(){
	int t;
	cin>>t;
	while(t--){
		ll n;
		cin>>n;
		cout<<uoc_max(n)<<endl;
	}
	return 0;
} 
