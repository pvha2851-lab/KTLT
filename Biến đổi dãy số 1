#include<bits/stdc++.h>
using namespace std;
int main(){
    int t;
    cin>>t;
    while(t--){
        int n;
        cin>>n;
        int a[n];
        for(int i=0;i<n;i++){
            cin>>a[i];
        }
        for(int i=0;i<n;i++){
            cout<<"[";
            for(int j=0;j<n-i;j++){
                cout<<a[j];
                if(j!=n-i-1)	cout<<" ";
            }
            cout<<"]\n";
            for(int j=0;j<n-i-1;j++){
                a[j]=a[j]+a[j+1];
            }
        }
    }
    return 0;
}
