#include<bits/stdc++.h>
using namespace std;
int n,k;
int cnt;
int a[200];
void Try(int i,int sum,int dem){
    if(sum==k&&dem==2){
        cnt++;
        return;
    }
    if(sum>k||dem>k)	return;
    for(int j=i;j<n;j++){
        if(sum+a[j]<=k&&dem<2){
            Try(j+1,sum+a[j],dem+1);
        }
    }
}
int main(){
    int t;
    cin>>t;
    while(t--){
        cin>>n>>k;
        cnt=0;
        for(int i=0;i<n;i++)	cin>>a[i];
        sort(a,a+n);
        Try(0,0,0);
        cout<<cnt<<endl;
    }
    return 0;
}
