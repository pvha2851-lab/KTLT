#include<bits/stdc++.h>
#define ll long long
using namespace std;
const ll mod=1e9+7;
int main(){
	int t;
	cin>>t;
	while(t--){
		ll n;
		cin>>n;
		vector<ll> dp(n+5,0);
		dp[0]=1;
		for(int c=1;c<=n;c++){
			for(int t=c;t<=n;t++){
				dp[t]=(dp[t]+dp[t-c])%mod;
			}
		}
		cout<<dp[n]-1<<endl;
	}
	return 0;
} 
