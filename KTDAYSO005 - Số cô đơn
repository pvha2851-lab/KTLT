#include<bits/stdc++.h>
using namespace std;
int main(){
    int t;
    cin>>t;
    while(t--){
        int n;
        cin>>n;
        map<int,int> cnt;
        for(int i=0;i<n;i++){
            int x;cin>>x;
            cnt[x]++;
        }
        int dem=0;
        for(auto x:cnt){
            if(x.second==1){
                dem++;
            }
        }
        cout<<dem<<endl;
    }
    return 0;
}
