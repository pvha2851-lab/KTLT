#include<bits/stdc++.h>
using namespace std;
int n,x;
int a[100];
vector<vector<int>> res;
void Try(int i,int sum,vector<int> v){
    if(sum==x){
        res.push_back(v);
        return;
    }
    if(sum>x)	return;
    for(int j=i;j<n;j++){
        if(sum+a[j]<=x){
            v.push_back(a[j]);
            Try(j,sum+a[j],v);
            v.pop_back();
        }
    }
}
int main(){
    int t;
    cin>>t;
    while(t--){
        cin>>n>>x;
        for(int i=0;i<n;i++){
            cin>>a[i];
        }
        sort(a,a+n);
        res.clear();
        vector<int> v;
        Try(0,0,v);
        if(res.empty())	cout<<-1<<endl;
        else{
            for(int i=0;i<res.size();i++){
                cout<<"[";
                for(int j=0;j<res[i].size();j++){
                    cout<<res[i][j];
                    if(j!=res[i].size()-1)	cout<<" ";
                }
                cout<<"]";
            }
            cout<<endl;
        }
    }
    return 0;
}
