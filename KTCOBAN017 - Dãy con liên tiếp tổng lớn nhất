#include<bits/stdc++.h>
#define ll long long
using namespace std;
int main(){
	int t;
	cin>>t;
	while(t--){
		ll n;
		cin>>n;
		vector<ll> a(n);
		for(int i=0;i<n;i++){
			cin>>a[i];
		}
		vector<ll> dp(n+5,0);
		dp[0]=a[0];
		ll dmax=a[0];
		for(int i=1;i<n;i++){
			dp[i]=max(a[i],dp[i-1]+a[i]);
			dmax=max(dmax,dp[i]);
		}
		cout<<dmax<<endl;
	}
	return 0;
}
