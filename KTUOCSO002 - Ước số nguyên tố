#include<bits/stdc++.h>
#define ll long long 
using namespace std;
ll cnt_uoc_prime(ll n){
	if(n==1)	return 0;
	ll cnt=0;
	for(int i=2;i<=sqrt(n);i++){
		if(n%i==0){
			cnt++;
			while(n%i==0){
				n/=i;
			}
		}
	} 
	if(n>1)	cnt++;
	return cnt;
} 
int main(){
	int t;
	cin>>t;
	while(t--){
		ll n;
		cin>>n;
		cout<<cnt_uoc_prime(n)<<endl; 
	} 
	return 0; 
} 
