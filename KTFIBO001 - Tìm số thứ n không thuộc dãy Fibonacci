#include<bits/stdc++.h>
#define ll long long
using namespace std;
const ll INF=1e18;
vector<ll> F;
void sangfibo(){
    F.push_back(1);
    F.push_back(1);
    while(1){
        ll x=F[F.size()-1]+F[F.size()-2];
        if(x>INF)	break;
        F.push_back(x);
    }
}
int main(){
    sangfibo();
    int t;
    cin>>t;
    while(t--){
        ll n;
        cin>>n;
        if(n==1){
            cout<<4<<endl;
            continue;
        }
        for(ll i=1;i<F.size();i++){
            if(F[i]-i>=n){
                cout<<n+i-1<<endl;
                break;
            }
        }
    }
    return 0;
}
