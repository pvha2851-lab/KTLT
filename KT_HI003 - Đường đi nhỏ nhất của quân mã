#include<bits/stdc++.h>
using namespace std;
int INF=1e6;
int n;
int x1,y1,x2,y2;
int a[100][100];
bool check[100][100];
int dx[8]={-1,1,1,-1,-2,2,2,-2};
int dy[8]={-2,2,-2,2,-1,1,-1,1};
int dmin;
void Try(int i,int j,int sum){
	if(i==x2&&j==y2){
		dmin=min(dmin,sum);
		return;
	}
	if(sum>dmin)	return;
	for(int k=0;k<8;k++){
		int x=i+dx[k];
		int y=j+dy[k];
		if(!check[x][y]&&x>=1&&x<=n&&y>=1&&y<=n){
			check[x][y]=true;
			Try(x,y,sum+a[x][y]);
			check[x][y]=false;
		}
	}
}
int main(){
	int t;
	cin>>t;
	while(t--){
		dmin=INF;
		cin>>n;
		for(int i=1;i<=n;i++){
			for(int j=1;j<=n;j++){
				cin>>a[i][j];
				check[i][j]=false;
			}
		}
		cin>>x1>>y1>>x2>>y2;
		check[x1][y1]=true;
		Try(x1,y1,a[x1][y1]);
		if(dmin==INF)	cout<<-1<<endl;
		else	cout<<dmin<<endl;
	}
	return 0;
} 
