#include <bits/stdc++.h>
using namespace std;
const int INF = 1e9;
int n;
int a[105][105];
int s, t, u, v;
int dx[] = {-1, 1, -1, 1, -2, 2, -2, 2};
int dy[] = {-2, 2, 2, -2, -1, 1, 1, -1};
int dijkstra() {
    vector<vector<int>> dist(n + 1, vector<int>(n + 1, INF));
    priority_queue<pair<int, pair<int, int>>, vector<pair<int, pair<int, int>>>, greater<>> pq;
    dist[s][t] = a[s][t];
    pq.push({a[s][t], {s, t}});
    while (!pq.empty()) {
        auto top = pq.top(); pq.pop();
        int cost = top.first;
        int x = top.second.first;
        int y = top.second.second;

        if (cost > dist[x][y]) continue;
        if (x == u && y == v) return cost;

        for (int k = 0; k < 8; k++) {
            int nx = x + dx[k];
            int ny = y + dy[k];
            if (nx >= 1 && nx <= n && ny >= 1 && ny <= n) {
                int newCost = cost + a[nx][ny];
                if (newCost < dist[nx][ny]) {
                    dist[nx][ny] = newCost;
                    pq.push({newCost, {nx, ny}});
                }
            }
        }
    }
    return -1;
}
int main() {
    int T;
    cin >> T;
    while (T--) {
        cin >> n;
        for (int i = 1; i <= n; i++)
            for (int j = 1; j <= n; j++)
                cin >> a[i][j];
        cin >> s >> t >> u >> v;

        int ans = dijkstra();
        cout << ans << '\n';
    }
    return 0;
}
