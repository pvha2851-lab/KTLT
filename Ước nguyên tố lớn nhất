#include<bits/stdc++.h>
#define ll long long
using namespace std;
ll uoc_max(ll n){
    if(n<=1)	return 0;
    ll dmax=0;
    while(n%2==0){
        dmax=2;
        n=n/2;
    }
    for(ll i=3;i*i<=n;i+=2){
        if(n%i==0){
            dmax=i;
            while(n%i==0){
                n=n/i;
            }
        }
    }
    if(n>1)	dmax=max(dmax,n);
    return dmax;
}
int main(){
    int t;
    cin>>t;
    while(t--){
        ll n;
        cin>>n;
        cout<<uoc_max(n)<<endl;
    }
    return 0;
}
