#include<bits/stdc++.h>
#define ll long long
using namespace std;
ll F[92];
char solve(ll n,ll k){
	if(n==1)	return 'A';
	if(n==2)	return 'B';
	if(k<=F[n-2]){
		return solve(n-2,k);
	}
	else{
		return solve(n-1,k-F[n-2]);
	}
}
int main(){
	F[1]=1;
	F[2]=1;
	for(int i=3;i<=92;i++){
		F[i]=F[i-2]+F[i-1];
	}
	int t;
	cin>>t;
	while(t--){
		ll n,k;
		cin>>n>>k;
		cout<<solve(n,k)<<endl;
	}
	return 0;
}
