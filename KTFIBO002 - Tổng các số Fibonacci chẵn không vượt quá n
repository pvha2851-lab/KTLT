#include<bits/stdc++.h>
#define ll long long
using namespace std;
const ll INF=1e9;
vector<ll> F;
void sangfibo(){
	F.push_back(1);
	F.push_back(2);
	while(1){
		ll x=F[F.size()-1]+F[F.size()-2];
		if(x>INF) break;
		F.push_back(x);
	}
}
int main(){
	sangfibo();
	int t;
	cin>>t;
	while(t--){
		ll n;
		cin>>n;
		ll sum=0;
		for(int i=0;i<F.size();i++){
			if(F[i]>n)	break;
			if(F[i]%2==0)	sum+=F[i];
		}
		cout<<sum<<endl;
	}
	return 0;
} 
