#include<bits/stdc++.h>
using namespace std;
int main(){
    int t;
    cin>>t;
    while(t--){
        int n;
        cin>>n;
        int a[n];
        for(int i=0;i<n;i++){
            cin>>a[i];
        }
        vector<int> v;
        vector<vector<int>> res;
        for(int i=0;i<n;i++){
            v.clear();
            for(int j=0;j<n-i;j++){
                v.push_back(a[j]);
            }
            for(int j=0;j<n-i-1;j++){
                a[j]=a[j]+a[j+1];
            }
            res.push_back(v);
        }
        for(int i=res.size()-1;i>=0;i--){
            cout<<"[";
            for(int j=0;j<res[i].size();j++){
                cout<<res[i][j];
                if(j!=res[i].size()-1)	cout<<" ";
            }
            cout<<"] ";
        }
        cout<<endl;
    }
    return 0;
}
