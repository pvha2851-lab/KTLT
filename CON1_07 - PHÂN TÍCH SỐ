#include<bits/stdc++.h>
using namespace std;
int n;
vector<vector<int>> res;
void Try(int i,int sum,vector<int> v){
    if(sum==n){
        res.push_back(v);
        return;
    }
    if(sum>n)	return;
    for(int j=i;j>=1;j--){
        if(sum+j<=n){
            v.push_back(j);
            Try(j,sum+j,v);
            v.pop_back();
        }
    }
}
int main(){
    int t;
    cin>>t;
    while(t--){
        cin>>n;
        res.clear();
        vector<int> v;
        Try(n,0,v);
        for(int i=0;i<res.size();i++){
            cout<<"(";
            for(int j=0;j<res[i].size();j++){
                cout<<res[i][j];
                if(j!=res[i].size()-1)	cout<<" ";
            }
            cout<<") ";
        }
        cout<<endl;
	}
    return 0;
}
