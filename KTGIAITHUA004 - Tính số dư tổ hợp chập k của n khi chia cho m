#include<bits/stdc++.h>
using namespace std;
int to_hop(int n,int k,int m){
	vector<int> C(k+1,0);
	C[0]=1;
	for(int i=1;i<=n;i++){
		for(int j=min(i,k);j>=0;j--){
			C[j]=(C[j]+C[j-1])%m;
		}
	} 
	return C[k];
}
int main(){
	int t;
	cin>>t;
	while(t--){
		int n,k,m;
		cin>>n>>k>>m;
		cout<<to_hop(n,k,m)<<endl; 
	} 
	return 0;
}
