#include<bits/stdc++.h>
using namespace std;
int main(){
    int t;
    cin>>t;
    cin.ignore();
    while(t--){
        string s;
        cin>>s;
        vector<int> a(26,0);
        for(char c:s){
            a[c-'a']++;
        }
        int pos=max_element(a.begin(),a.end())-a.begin();
        int cnt=a[pos];
        if(s.size()%2==0){
            if(cnt>s.size()/2)	cout<<-1;
            else	cout<<1;
        }
        else{
            if(cnt>s.size()/2+1)	cout<<-1;
            else	cout<<1;
        }
        cout<<endl;
    }
    return 0;
}
