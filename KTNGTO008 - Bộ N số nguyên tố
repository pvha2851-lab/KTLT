#include<bits/stdc++.h>
#define ll long long
using namespace std;
int p,n,s;
vector<vector<int>> res;
vector<int> isprime(1000,1);
vector<int> prime;
void sangnguyento(){
	for(int i=2;i<=sqrt(1000);i++){
		if(isprime[i]){
			for(int j=i*i;j<=1000;j+=i)
				isprime[j]=0;
		}
	}
	for(int i=2;i<=1000;i++){
		if(isprime[i]){
			prime.push_back(i);
		}
	}
}
void Try(int i,int k,int sum,vector<int> v){
	if(sum==s&&k==n){
		res.push_back(v);
		return;
	}
	if(sum>s||k>n)	return;
	for(ll j=i;j<prime.size();j++){
		if(sum+prime[j]<=s&&k<n){
			v.push_back(prime[j]);
			Try(j+1,k+1,sum+prime[j],v);
			v.pop_back();
		}
	}
}
int main(){
	sangnguyento();
	cin>>p>>n>>s;
	vector<int> v;
	int pos=upper_bound(prime.begin(),prime.end(),p)-prime.begin();
	Try(pos,0,0,v);
	if(res.size()==0)	cout<<-1;
	else{
		for(int i=0;i<res.size();i++){
			for(int j=0;j<res[i].size();j++){
				cout<<res[i][j]<<" ";
			}
			cout<<endl;
		}
	}
	return 0;
} 
