#include<bits/stdc++.h>
using namespace std;
vector<int> prime;
vector<int> isprime(201,1);
vector<vector<int>> res;
int n,p,s;
void sangnguyento(){
    for(int i=2;i*i<=200;i++){
        if(isprime[i]){
            for(int j=i*i;j<=200;j+=i){
                isprime[j]=0;
            }
        }
    }
    for(int i=2;i<=200;i++){
        if(isprime[i])	prime.push_back(i);
    }
}
void Try(int i,int k,int sum,vector<int> v){
    if(sum==s&&k==n){
        res.push_back(v);
        return ;
    }
    if(sum>s||k>n)	return;
    for(int j=i;j<prime.size();j++){
        if(sum+prime[j]<=s&&k<n){
            v.push_back(prime[j]);
            Try(j+1,k+1,sum+prime[j],v);
            v.pop_back();
        }
    }
}
int main(){
    sangnguyento();
    int t;
    cin>>t;
    while(t--){
        res.clear();
        cin>>n>>p>>s;
        int pos=upper_bound(prime.begin(),prime.end(),p)-prime.begin();
        vector<int> v;
        Try(pos,0,0,v);
        cout<<res.size()<<endl;
        for(int i=0;i<res.size();i++){
            for(int j=0;j<res[i].size();j++){
                cout<<res[i][j]<<" ";
            }
            cout<<endl;
        }
	}
    return 0;
}
